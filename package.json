{
  "name": "vite-react-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint \"src/**/*.{js,ts,jsx}\"--report-unused-disable-directives --max-warnings 0",
